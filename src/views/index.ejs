<html>
	<head>
		<title>Thanks for dropping in! ðŸ‘‹</title>
		<meta charset="utf-8" />
		<meta name="viewport" content="width=device-width, initial-scale=1, user-scalable=no" />
        <link rel="stylesheet" href="../../css/main.css" />
        <link rel="stylesheet" href="../../css/styles.css" />
        <noscript><link rel="stylesheet" href="../../css/noscript.css" /></noscript>
        <link rel="icon" type="image/x-icon" href="../images/favicon.png" />
	</head>
	<body class="is-preload">

		<!-- Wrapper-->
			<div id="wrapper">

				<!-- Nav -->
					<nav id="nav">
						<a href="#" class="icon solid fa-home"><span>Home</span></a>
						<a href="#work" class="icon solid fa-folder"><span>Work</span></a>
						<a href="#contact" class="icon solid fa-envelope"><span>Contact</span></a>
						<a href="https://github.com/siiiiilvio" class="icon brands fa-github"><span>Github</span></a>
					</nav>

				<!-- Main -->
					<div id="main">

						<!-- Me -->
							<article id="home" class="panel intro">
								<header>
									<h1><%- profile.name %></h1>
									<p><%- profile.headline %></p>
								</header>
								<a href="#work" class="jumplink pic">
									<span class="arrow icon solid fa-chevron-right"></span>
									<img src="../../images/me.jpg" alt="" />
								</a>
							</article>

						<!-- Work -->
							<article id="work" class="panel work_sections_class">
                                <section>
                                    <header>
                                        <h2><i class="fas fa-user"></i> <%- profile.about.aboutHeading %></h2>
                                    </header>
                                    <p><%- profile.about.aboutContent %></p>
                                </section>
                                <section>
                                    <header>
                                        <h2><i class="fas fa-briefcase"></i> <%- profile.experience.experienceHeading %></h2>
                                    </header>
                                    <% profile.experience.experienceArray.forEach((experience, index) => { %>
                                        <div class="ex_container">
                                            <div class="ex_img">
                                                <img src='../../images/experience/<%= index %>.jpg' onerror="this.src='../../images/default.jpg'"/>
                                            </div>
                                            <%if (experience.experienceGroupArray) { %>
                                                <div class="ex_text no_margin">
                                                    <p class="ex_ttl"><%- experience.company %></p>
                                                    <p><%- experience.date %></p>
                                                    <ul>
                                                        <% experience.experienceGroupArray.forEach((groupExperience) => { %>
                                                            <li>
                                                                <p class="ex_ttl"><%- groupExperience.experienceTitle %></p> 
                                                                <p class="ex_dur"><%- groupExperience.date %></p>
                                                                <p class="ex_desc"><%- groupExperience.description %></p> 
                                                            </li>
                                                        <% }); %>
                                                    </ul>
                                                </div>
                                            <%} else { %>
                                                <div class="ex_text">
                                                    <p>
                                                        <p class="ex_ttl"><%- experience.experienceTitle %></p>
                                                        <p class="ex_com"><%- experience.company %></p>
                                                        <p class="ex_dur"><%- experience.date %> | <%- experience.duration %></p>
                                                        <p class="ex_loc"><%- experience.location %></p>
                                                        <p class="ex_desc"><%- experience.description %></p>
                                                    </p>
                                                </div>
                                            <% } %>
                                        </div>
                                    <% }); %>
                                </section>
                                <section>
                                    <header>
                                        <h2><i class="fas fa-graduation-cap"></i> <%- profile.education.educationHeading %></h2>
                                    </header>
                                    <% profile.education.educationArray.forEach((education, index) => { %>
                                        <div class="edu_container">
											<div class="edu_img">
                                                <img src='../../images/education/<%= index %>.jpg' onerror="this.src='../../images/default.jpg'"/>
                                            </div>
                                            <div class="edu_text">
                                                <p class="ex_ttl"><%- education.school %></p>
                                                <% education.fieldsArray.forEach((field, index) => { %>
                                                    <span><%- field %></span>
                                                    <%if (index !== education.fieldsArray.length - 1) { %>
                                                        <span> | </span>
                                                    <% } %>
                                                <% }); %>
                                                <p><%- education.time %></p>
                                                <p><%- education.society %></p>
                                                <p><%- education.curriculum %></p>
                                            </div>
                                        </div>
                                    <% }); %>
                                </section>
                                <section>
                                    <header>
                                        <h2><i class="fas fa-stamp"></i> <%- profile.certifications.certificationsHeading %></h2>
                                    </header>
                                    <% profile.certifications.certificationsArray.forEach((certification, index) => { %>
                                        <div class="lic_container">
                                            <div class="lic_img">
                                                <img src='../../images/certifications/0.jpg' onerror="this.src='../../images/default.jpg'"/>
                                            </div>
                                            <div class="lic_text">
                                                <p>
                                                    <p><%- certification.certification %></p>
                                                    <p><%- certification.subtitle %></p>
                                                    <p><%- certification.credetials %></p>
                                                </p>
                                            </div>
                                        </div>
                                    <% }); %>
                                </section>
                                <section>
                                    <header>
                                        <h2><i class="fas fa-language"></i> <%- profile.languages.languagesHeading %></h2>
                                    </header>
                                        <% profile.languages.languagesArray.forEach((language, index) => { %>
                                            <div class="sngl_lng">
                                                <p class="ex_ttl"><%- language.language %></p>
                                                <p><%- language.proficiency %></p>
                                            </div>
                                        <% }); %>
                                </section>
							</article>

						<!-- Contact -->
							<article id="contact" class="panel">
								<header>
                                    <%if (query && query.message === 'thanks') { %>
                                        <p style="color:salmon">Gee thanks! I'll get back to you right away!!</p>
                                        <br>
                                    <% } %>
									<h2>Contact Me</h2>
								</header>
								<form id="contact" action="/contact" method="POST">
									<div>
										<div class="row">
											<div class="col-6 col-12-medium">
												<input type="text" name="name" placeholder="Name" required />
											</div>
											<div class="col-6 col-12-medium">
												<input type="text" name="email" placeholder="Email" required />
											</div>
											<div class="col-12">
												<input type="text" name="subject" placeholder="Subject" required />
											</div>
											<div class="col-12">
												<textarea name="message" placeholder="Message" rows="6"></textarea>
											</div>
											<div class="col-12">
												<input type="submit" value="Send Message" />
											</div>
										</div>
									</div>
								</form>
							</article>

					</div>

				<!-- Footer -->
					<div id="footer">
						<ul class="copyright">
							<li>This site was auto-generated using my <a href="https://www.linkedin.com/in/silvio-ciarrocchi-133a6213/"><img width="15" height="15" src='../images/linkedIn.ico' /></a> profile</li><li>Design: <a href="http://html5up.net">HTML5 UP</a></li>
						</ul>
					</div>

			</div>

		<!-- Scripts -->
			<script src="../../js/jquery.min.js"></script>
			<script src="../../js/browser.min.js"></script>
			<script src="../../js/breakpoints.min.js"></script>
			<script src="../../js/util.js"></script>
			<script src="../../js/main.js"></script>
	</body>
</html>